{
  "name": "typescript-webpack2-starter",
  "version": "0.0.1",
  "description": "fork of angular2-webpack-starter and remove the angular2 part, so it's a clear typesscript and webpack starter seed",
  "keywords": [
    "webpack",
    "typescript"
  ],
  "author": "Patrick Stapleton <patrick@angularclass.com>, Peter Denev <peter.denev@gmail.com>",
  "homepage": "https://github.com/peterdenev/typescript-webpack2-starter",
  "license": "MIT",
  "scripts": {
    "build:dev": "npm run clean:dist && webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "npm run clean:dist && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build": "npm run build:dev",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "rimraf": "rimraf",
    "version": "npm run build",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch": "npm run watch:dev",
    "webpack": "webpack"
  },
  "dependencies": {
    "jquery": "^3.2.0",
    "core-js": "^2.4.1"
  },
  "devDependencies": {
    "@types/jquery": "^2.0.41",
    "@types/node": "^7.0.0",
    "@types/ractive": "^0.7.27",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.0.27",
    "@types/webpack": "^2.0.0",
    "assets-webpack-plugin": "^3.5.1",
    "awesome-typescript-loader": "^3.1.2",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.26.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "~2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "find-root": "^1.0.0",
    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.0",
    "json-loader": "^0.5.4",
    "optimize-js-plugin": "0.0.4",
    "raw-loader": "0.5.1",
    "rimraf": "~2.6.0",
    "sass-loader": "^6.0.0",
    "script-ext-html-webpack-plugin": "^1.6.2",
    "source-map-loader": "^0.2.0",
    "string-replace-loader": "1.0.5",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "ts-loader": "^2.0.1",
    "ts-node": "^2.0.0",
    "tslib": "^1.5.0",
    "tslint": "~4.5.1",
    "tslint-loader": "^3.3.0",
    "typescript": "~2.2.1",
    "url-loader": "^0.5.7",
    "webpack": "2.2.0",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-merge": "~3.0.0"
  },
  "repository": {},
  "bugs": {},
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
